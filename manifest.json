{
  "author": "igorlogius",
  "default_locale": "en",
  "homepage_url": "https://github.com/igorlogius/copy-tabs",
  "background": {
	"scripts": ["background.js"]
  },
  "browser_action": {
    "theme_icons": [
      {
        "light": "dark.png",
        "dark": "light.png",
        "size": 128
      }
    ]
  },
  "commands": {
    "cpyalllnk": {
      "description": "__MSG_cpyalllnk__",
      "suggested_key": {
        "default": "F1"
      }
    },
    "cpyalltxt": {
      "description": "__MSG_cpyalltxt__",
      "suggested_key": {
        "default": "F2"
      }
    },
    "cpysellnk": {
      "description": "__MSG_cpysellnk__",
      "suggested_key": {
        "default": "F3"
      }
    },
    "cpyseltxt": {
      "description": "__MSG_cpyseltxt__",
      "suggested_key": {
        "default": "F4"
      }
    }
  },
  "default_area": "navbar",
  "icons": {
    "256": "icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_addonName__",
  "description": "__MSG_addonDesc__",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "notifications",
    "clipboardWrite",
    "storage",
    "menus",
    "tabs"
  ],
  "version": "1.12.32"
}
