{
  "author": "igorlogius",
  "background": {
	"scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "dark-128.png",
        "dark": "light-128.png",
        "size": 128
      },
      {
        "light": "dark-64.png",
        "dark": "light-64.png",
        "size": 64
      },
      {
        "light": "dark-32.png",
        "dark": "light-32.png",
        "size": 32
      }
    ]
  },
  "commands": {
    "copy-action": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      }
    }
  },
  "icons": {
    "256": "icon.svg"
  },
  "manifest_version": 2,
  "name": "Copy TabURLs as HTML",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "menus",
    "storage",
    "tabs",
    "clipboardWrite"
  ],
  "version": "1.7.12"
}
