{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/copy-taburls",
  "background": {
	"scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "dark.png",
        "dark": "light.png",
        "size": 128
      }
    ]
  },
  "commands": {
    "cpyallhtm": {
      "description": "Copy All as Links (🔗)",
      "suggested_key": {
        "default": "F1"
      }
    },
    "cpyalltxt": {
      "description": "Copy All as Text (✏️)",
      "suggested_key": {
        "default": "F2"
      }
    },
    "cpyselhtm": {
      "description": "Copy Selected as Links (🔗)",
      "suggested_key": {
        "default": "F3"
      }
    },
    "cpyseltxt": {
      "description": "Copy Selected as Text (✏️)",
      "suggested_key": {
        "default": "F4"
      }
    },
    "savallhtm": {
      "description": "Save All as Links (🔗)",
      "suggested_key": {
        "default": "F6"
      }
    },
    "savalltxt": {
      "description": "Save All as Text (✏️)",
      "suggested_key": {
        "default": "F7"
      }
    },
    "savselhtm": {
      "description": "Save Selected as Links (🔗)",
      "suggested_key": {
        "default": "F8"
      }
    },
    "savseltxt": {
      "description": "Save Selected as Text (✏️)",
      "suggested_key": {
        "default": "F9"
      }
    }
  },
  "icons": {
    "256": "icon.svg"
  },
  "manifest_version": 2,
  "name": "Copy TabURLs",
  "permissions": [
    "notifications",
    "clipboardWrite",
    "tabs"
  ],
  "version": "1.9.20"
}
