{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/copy-taburls",
  "background": {
	"scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "dark.png",
        "dark": "light.png",
        "size": 128
      }
    ]
  },
  "commands": {
    "cpyallhtm": {
      "description": "Copy All Tabs as Links (üîó)",
      "suggested_key": {
        "default": "F1"
      }
    },
    "cpyalltxt": {
      "description": "Copy All Tabs as Text (‚úèÔ∏è)",
      "suggested_key": {
        "default": "F2"
      }
    },
    "cpyselhtm": {
      "description": "Copy Selected Tabs as Links (üîó)",
      "suggested_key": {
        "default": "F3"
      }
    },
    "cpyseltxt": {
      "description": "Copy Selected Tabs as Text (‚úèÔ∏è)",
      "suggested_key": {
        "default": "F4"
      }
    }
  },
  "icons": {
    "256": "icon.svg"
  },
  "manifest_version": 2,
  "name": "Copy TabURLs",
  "description": "Copy the URLs of tabs as hyperlinks or plaintext",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "menus",
